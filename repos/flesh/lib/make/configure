  OPENMP=`echo $OPENMP | tr '[:upper:]' '[:lower:]'`
  
  if test "$OPENMP" != 'yes' -a "$OPENMP" != 'no' ; then
    { echo "configure: error: Didn't recognize setting of OPENMP=\"$OPENMP\" (should be either \"yes\" or \"no\")" 1>&2; exit 1; }
  fi
  OPENMP_MODE=$OPENMP
